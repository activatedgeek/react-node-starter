{
  "extends": "airbnb",
  "rules": {
    "arrow-body-style": [0],
    "comma-dangle": [0],
    "guard-for-in": [1],
    "jsx-a11y/img-has-alt": [1],
    "max-len": [1],
    "no-loop-func": [1],
    "no-param-reassign": [1],
    "no-restricted-syntax": [1],
    "no-underscore-dangle": [0],
    "no-unneeded-ternary": [1],
    "react/sort-comp": [0]
  }
}
