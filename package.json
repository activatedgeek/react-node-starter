{
  "name": "react-node-starter",
  "version": "0.1.0",
  "scripts": {
    "bower": "bower install",
    "build": "CMD=webpack;[ \"$NODE_ENV\" = production ] && CMD=\"$CMD -p\";eval $CMD;",
    "postinstall": "npm prune",
    "start": "node ./bin/www;",
    "test": "NODE_ENV=testing mocha --recursive tests/",
    "watch:build": "webpack --verbose --progress --watch",
    "watch:start": "nodemon ./bin/www"
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "bunyan": "^1.8.0",
    "bunyan-middleware": "^0.3.1",
    "compression": "^1.5.2",
    "connect-redis": "^3.0.2",
    "cookie-parser": "^1.3.5",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "jade": "^1.11.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "redis": "^2.6.0-1",
    "superagent": "^1.8.1"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.6.0",
    "bower": "^1.7.7",
    "chai": "^3.5.0",
    "enzyme": "^2.1.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.2.3",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.1",
    "supertest": "^1.2.0",
    "webpack": "^1.12.14"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:activatedgeek/react-node-starter.git"
  },
  "license": "MIT"
}
