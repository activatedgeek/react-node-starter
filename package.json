{
  "name": "react-node-starter",
  "version": "0.1.0",
  "scripts": {
    "bower": "bower install",
    "build": "CMD=webpack;[ \"$NODE_ENV\" = production ] && CMD=\"$CMD -p\";eval $CMD;",
    "postinstall": "bower install && npm run prune",
    "prune": "CMD=\"npm prune\";[ \"$NODE_ENV\" = production ] && CMD=\"$CMD --production\";eval $CMD;",
    "start": "node ./bin/www;",
    "test": "NODE_ENV=testing mocha --recursive tests/",
    "watch:build": "webpack --verbose --progress --watch",
    "watch:start": "nodemon ./bin/www"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "bunyan": "^1.8.0",
    "bunyan-middleware": "^0.3.1",
    "compression": "^1.6.2",
    "connect-redis": "^3.1.0",
    "cookie-parser": "^1.4.3",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "jade": "^1.11.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "redis": "^2.6.2",
    "superagent": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.11.6",
    "bower": "^1.7.7",
    "chai": "^3.5.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "mocha": "^3.0.0",
    "nodemon": "^1.10.0",
    "supertest": "^2.0.0",
    "webpack": "^1.13.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:activatedgeek/react-node-starter.git"
  },
  "license": "MIT"
}
